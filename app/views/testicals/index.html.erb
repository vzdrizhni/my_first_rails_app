<p id="notice"><%= notice %></p>

<h1>Testicals</h1>

<table>
  <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testicals.each do |testical| %>
      <tr>
        <td><%= testical.make %></td>
        <td><%= testical.model %></td>
        <td><%= testical.year %></td>
        <td><%= link_to 'Show', testical %></td>
        <td><%= link_to 'Edit', edit_testical_path(testical) %></td>
        <td><%= link_to 'Destroy', testical, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testical', new_testical_path %>
